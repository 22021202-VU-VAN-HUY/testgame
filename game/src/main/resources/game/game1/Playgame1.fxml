<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="522.0" prefWidth="789.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.game.game1.Game1Controller">
    <children>
        <Button fx:id="Backfromplaygametogame1" layoutY="1.0" mnemonicParsing="false" onMouseClicked="#Backtogame1" text="Back" />
      <VBox layoutX="171.0" layoutY="114.0" prefHeight="183.0" prefWidth="447.0" spacing="10.0">
         <children>
            <Label fx:id="questiongame" prefHeight="57.0" prefWidth="446.0" text="Question" wrapText="true" />
            <CheckBox fx:id="a" mnemonicParsing="false" onMouseClicked="#ChooseanswerA" prefHeight="17.0" prefWidth="443.0" text="CheckBox" wrapText="true" />
            <CheckBox fx:id="b" mnemonicParsing="false" onMouseClicked="#ChooseanswerB" prefHeight="17.0" prefWidth="446.0" text="CheckBox" wrapText="true" />
            <CheckBox fx:id="c" mnemonicParsing="false" onMouseClicked="#ChooseanswerC" prefHeight="17.0" prefWidth="444.0" text="CheckBox" wrapText="true" />
            <CheckBox fx:id="d" mnemonicParsing="false" onMouseClicked="#ChooseanswerD" prefHeight="17.0" prefWidth="474.0" text="CheckBox" wrapText="true" />
         </children>
      </VBox>
      <HBox layoutX="252.0" layoutY="313.0" prefHeight="25.0" prefWidth="285.0" spacing="15.0">
         <children>
            <Button fx:id="back_question" mnemonicParsing="false" onMouseClicked="#Back_question" text="&lt;-" />
            <Label fx:id="score" prefHeight="21.0" prefWidth="119.0" text="Score:">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <Button fx:id="submitanswer" mnemonicParsing="false" onMouseClicked="#Submitanswer" text="Submit" />
            <Button fx:id="next_question" mnemonicParsing="false" onMouseClicked="#Next_question" text="-&gt;" />
         </children>
      </HBox>
    </children>
</AnchorPane>
